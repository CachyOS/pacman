msgstr "  -r, --recv-keys           Obter as keyids especificadas"

#: scripts/pacman-key.sh.in:69
msgid "  -u, --updatedb            Update the trustdb of pacman"
msgstr "  -u, --updatedb            Atualiza a trustdb do pacman"

