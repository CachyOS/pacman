msgid "Operations:"
msgstr "Oberatadennoù :"

#: scripts/pacman-key.sh.in:63
msgid "  -a, --add                 Add the specified keys (empty for stdin)"
msgstr "-a, --add Ouzhpennañ an alc'hwezioù erspizet (goulo evit stdin)"

